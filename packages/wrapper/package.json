{
  "name": "@agent-relay/wrapper",
  "version": "0.1.0",
  "description": "CLI agent wrappers for Agent Relay - tmux, pty integration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./id-generator": {
      "types": "./dist/id-generator.d.ts",
      "import": "./dist/id-generator.js"
    },
    "./tmux-resolver": {
      "types": "./dist/tmux-resolver.d.ts",
      "import": "./dist/tmux-resolver.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agent-relay/api-types": "*",
    "@agent-relay/protocol": "0.1.0",
    "@agent-relay/config": "0.1.0",
    "@agent-relay/continuity": "0.1.0",
    "@agent-relay/resiliency": "0.1.0"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "@agent-relay/sdk": "*"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "agent-relay",
    "wrapper",
    "tmux",
    "pty",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AgentWorkforce/relay.git",
    "directory": "packages/wrapper"
  },
  "license": "MIT"
}
