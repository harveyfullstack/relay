{
  "name": "@agent-relay/wrapper",
  "version": "2.1.4",
  "description": "CLI agent wrappers for Agent Relay - tmux, pty integration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./id-generator": {
      "types": "./dist/id-generator.d.ts",
      "import": "./dist/id-generator.js",
      "default": "./dist/id-generator.js"
    },
    "./tmux-resolver": {
      "types": "./dist/tmux-resolver.d.ts",
      "import": "./dist/tmux-resolver.js",
      "default": "./dist/tmux-resolver.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agent-relay/api-types": "2.1.4",
    "@agent-relay/protocol": "2.1.4",
    "@agent-relay/config": "2.1.4",
    "@agent-relay/continuity": "2.1.4",
    "@agent-relay/resiliency": "2.1.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitest": "^3.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "agent-relay",
    "wrapper",
    "tmux",
    "pty",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgentWorkforce/relay.git",
    "directory": "packages/wrapper"
  },
  "license": "Apache-2.0"
}
