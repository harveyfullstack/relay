{
  "name": "@agent-relay/storage",
  "version": "2.0.16",
  "description": "Storage adapters and interfaces for Relay message/session persistence",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./adapter": {
      "types": "./dist/adapter.d.ts",
      "import": "./dist/adapter.js",
      "default": "./dist/adapter.js"
    },
    "./sqlite-adapter": {
      "types": "./dist/sqlite-adapter.d.ts",
      "import": "./dist/sqlite-adapter.js",
      "default": "./dist/sqlite-adapter.js"
    },
    "./batched-sqlite-adapter": {
      "types": "./dist/batched-sqlite-adapter.d.ts",
      "import": "./dist/batched-sqlite-adapter.js",
      "default": "./dist/batched-sqlite-adapter.js"
    },
    "./dlq-adapter": {
      "types": "./dist/dlq-adapter.d.ts",
      "import": "./dist/dlq-adapter.js",
      "default": "./dist/dlq-adapter.js"
    },
    "./dead-letter-queue": {
      "types": "./dist/dead-letter-queue.d.ts",
      "import": "./dist/dead-letter-queue.js",
      "default": "./dist/dead-letter-queue.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "better-sqlite3": "^12.6.2"
  },
  "peerDependenciesMeta": {
    "better-sqlite3": {
      "optional": true
    }
  },
  "dependencies": {
    "@agent-relay/protocol": "2.0.16"
  },
  "devDependencies": {
    "@types/node": "^22.19.3",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgentWorkforce/relay.git",
    "directory": "packages/storage"
  }
}
