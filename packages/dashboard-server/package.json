    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@agent-relay/protocol": "*",
    "@agent-relay/config": "*",
    "@agent-relay/storage": "*",
    "@agent-relay/bridge": "*",
    "@agent-relay/utils": "*",
    "@agent-relay/resiliency": "*",
    "@agent-relay/trajectory": "*",
    "@agent-relay/cloud": "*",
    "@agent-relay/daemon": "*",
    "@agent-relay/user-directory": "*",
    "@agent-relay/sdk": "*",
    "express": "^4.21.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.19.3",
    "@types/ws": "^8.18.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
