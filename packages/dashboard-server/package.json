{
  "name": "@agent-relay/dashboard-server",
  "version": "0.1.0",
  "description": "Relay dashboard server - HTTP/WebSocket server for agent coordination",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
