{
  "name": "@agent-relay/spawner",
  "version": "2.0.19",
  "description": "Agent spawning types and utilities for Agent Relay",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "default": "./dist/types.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitest": "^3.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "agent-relay",
    "spawner",
    "agent-lifecycle"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgentWorkforce/relay.git",
    "directory": "packages/spawner"
  },
  "license": "Apache-2.0"
}
